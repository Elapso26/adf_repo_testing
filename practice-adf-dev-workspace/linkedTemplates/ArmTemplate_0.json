{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "practice-adf-dev-workspace"
		},
		"dataFactory_properties_globalParameters_environment_value": {
			"type": "string",
			"defaultValue": "dev"
		},
		"dataFactory_location": {
			"type": "string",
			"defaultValue": "centralindia"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[parameters('factoryName')]",
			"type": "Microsoft.DataFactory/factories",
			"apiVersion": "2018-06-01",
			"properties": {
				"globalParameters": {
					"environment": {
						"type": "string",
						"value": "[parameters('dataFactory_properties_globalParameters_environment_value')]"
					}
				}
			},
			"dependsOn": [],
			"location": "[parameters('dataFactory_location')]"
		}
	]
}